{
    "name": "xtend-laravel/addon-quiz-app",
    "description": "Quiz App Addon for XtendLunar",
    "type": "library",
    "keywords": [
        "laravel",
        "xtendlunar"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "XtendLunar\\Addons\\QuizApp\\": "src/",
            "QuizApp\\Database\\Factories\\": "database/factories/",
            "QuizApp\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "authors": [
        {
            "name": "Adam Lee",
            "email": "adam@codelabx.ltd"
        }
    ],
    "require": {
        "php": "^8.1",
    },
    "extra": {
        "laravel": {
            "providers": [
                "XtendLunar\\Addons\\QuizApp\\QuizAppProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        }
    }
}
